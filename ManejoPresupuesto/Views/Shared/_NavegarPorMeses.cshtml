@model DateTime

<h4>
    <a asp-route-mes="@ViewBag.mesAnterior" asp-route-annio="@ViewBag.annioAnterior" class="btn btn-secondary"><i class="bi-chevron-left"></i></a>
    @Model.ToString("MMM")-@Model.Year
    <a asp-route-mes="@ViewBag.mesPosterior" asp-route-annio="@ViewBag.annioPosterior" class="btn btn-secondary"><i class="bi-chevron-right"></i></a>
</h4>